function CTest_LogObjectList(objects) {

	//console.log("objects.size() = " + objects.size() );
	
	if (objects != null) {
		var i = 0;
		
		objects.each(function(o) {
			i++;
			console.log("   " + i + ": " + o.name + " (" + o.id + ")" );	
		});
	}


}


